.cateGorysListContainer .header {
  width: 100%;
  height: 55rpx;
  padding: 10rpx 0;
}
.cateGorysListContainer .header .search {
  width: 90%;
  height: 100%;
  margin: 0 auto;
  background: #eee;
  border-radius: 10rpx;
  line-height: 55rpx;
  text-align: center;
  font-size: 26rpx;
}
.cateGorysListContainer .contentContainer {
  border-top: 1rpx solid #eee;
  height: calc(100vh - 76rpx);
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.cateGorysListContainer .contentContainer .leftContainer {
  width: 20%;
  height: 100%;
  border-right: 1rpx solid #333;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.cateGorysListContainer .contentContainer .leftContainer .navItem {
  position: relative;
  font-size: 26rpx;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
}
.cateGorysListContainer .contentContainer .leftContainer .navItem.active {
  color: #ab2b2b;
}
.cateGorysListContainer .contentContainer .leftContainer .navItem.active:after {
  position: absolute;
  left: 2rpx;
  top: 15%;
  content: '';
  width: 2rpx;
  height: 70%;
  background: #ab2b2b;
}
.cateGorysListContainer .contentContainer .rightContainer {
  width: 80%;
  height: 100%;
}
