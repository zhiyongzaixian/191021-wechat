.videoContainer .header {
  display: flex;
  padding: 10rpx;
}


.videoContainer .header image {
  width: 60rpx;
  height: 60rpx;
}


.videoContainer .search {
  border: 1rpx solid #eee;
  border-radius: 6rpx;
  flex: 1;
  margin: 0 20rpx;
}

.videoContainer .search  input {
  width: 100%;
  height: 100%;
}


.videoContainer .search .placeholder {
  font-size: 28rpx;
  text-align: center;
  color: #d43c33;
}
