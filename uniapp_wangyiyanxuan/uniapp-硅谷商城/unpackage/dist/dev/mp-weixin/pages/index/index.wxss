#indexContainer .header {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  height: 60rpx;
  width: 100%;
  padding: 10rpx 0;
}
#indexContainer .header .logo {
  width: 140rpx;
  height: 40rpx;
  margin: 10rpx 30rpx;
}
#indexContainer .header .searchInput {
  position: relative;
  height: 60rpx;
  width: 420rpx;
  background: #ededed;
}
#indexContainer .header .searchInput .iconfont {
  position: absolute;
  left: 10rpx;
  top: 15rpx;
  font-size: 30rpx;
}
#indexContainer .header .searchInput input {
  width: 370rpx;
  margin-left: 50rpx;
  height: 100%;
  color: #bb2c08;
}
#indexContainer .header .searchInput .placeholder {
  font-size: 28rpx;
  text-align: center;
  line-height: 60rpx;
}
#indexContainer .header button {
  font-size: 24rpx;
  width: 144rpx;
  height: 60rpx;
  margin: 0 10rpx;
  color: #b4282d;
  white-space: nowrap;
  overflow: hidden;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
  padding: 0 5rpx;
}
.testxxxxxxxxxxxxxxxxxxxxxxxxxxxx {
  font-size: 0;
}

